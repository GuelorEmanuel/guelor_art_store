<h1>Listing Products</h1>
<div class="outer">
  <%= for product <- @products do %>
    <div class="container cf">
      <a href="<%= Routes.product_path(@conn, :show, product) %>" class="product hentry author-iddris-sandu post-type-store-item article-index-1 featured clear" id="thumb-rastaclat-x-iddris-sandu-limited-edition-grad-clat" data-item-id="5dfd31bf2b533014b7b344b0">
        <div>
            <div class="product-image sqs-pinterest-image">
              <div class="intrinsic">
                  <div style="overflow: hidden;">
                    <img data-src="https://images.squarespace-cdn.com/content/v1/5a76ae3251a584e6151d0f99/1576874720720-WG3MJR37TGJBRVRUBHQY/ke17ZwdGBToddI8pDm48kLkXF2pIyv_F2eUT9F60jBl7gQa3H78H3Y0txjaiv_0fDoOvxcdMmMKkDsyUqMSsMWxHk725yiiHCCLfrh8O1z4YTzHvnKhyp6Da-NYroOW3ZGjoBKy3azqku80C789l0iyqMbMesKd95J-X4EagrgU9L3Sa3U8cogeb0tjXbfawd0urKshkc5MgdBeJmALQKw/Iddris1242Iddris+Final+14.jpg" data-image="https://images.squarespace-cdn.com/content/v1/5a76ae3251a584e6151d0f99/1576874720720-WG3MJR37TGJBRVRUBHQY/ke17ZwdGBToddI8pDm48kLkXF2pIyv_F2eUT9F60jBl7gQa3H78H3Y0txjaiv_0fDoOvxcdMmMKkDsyUqMSsMWxHk725yiiHCCLfrh8O1z4YTzHvnKhyp6Da-NYroOW3ZGjoBKy3azqku80C789l0iyqMbMesKd95J-X4EagrgU9L3Sa3U8cogeb0tjXbfawd0urKshkc5MgdBeJmALQKw/Iddris1242Iddris+Final+14.jpg" data-image-dimensions="2500x1667" data-image-focal-point="0.5,0.5" data-load="false" alt="Iddris1242Iddris Final 14.jpg" style="font-size: 0px; left: -71.707px; top: 0px; width: 430.414px; height: 287px; position: relative;" class="image loaded" data-image-resolution="2500w" src="https://images.squarespace-cdn.com/content/v1/5a76ae3251a584e6151d0f99/1576874720720-WG3MJR37TGJBRVRUBHQY/ke17ZwdGBToddI8pDm48kLkXF2pIyv_F2eUT9F60jBl7gQa3H78H3Y0txjaiv_0fDoOvxcdMmMKkDsyUqMSsMWxHk725yiiHCCLfrh8O1z4YTzHvnKhyp6Da-NYroOW3ZGjoBKy3azqku80C789l0iyqMbMesKd95J-X4EagrgU9L3Sa3U8cogeb0tjXbfawd0urKshkc5MgdBeJmALQKw/Iddris1242Iddris+Final+14.jpg?format=2500w" data-parent-ratio="1.0">
                  </div>
              </div>
            </div>
            <div class="product-overlay">
              <div class="product-meta">
                  <div class="product-title"><%= product.name %></div>
                  <div class="product-detail"><%= product.detail %></div>
                  <div class="product-price">
                    <span class="sqs-money-native"><%= product.price %></span>
                  </div>
              </div>
            </div>
            <%= if product.quantity == 0 do %>
               <div class="product-mark sold-out">sold out</div>
            <% end %>
        </div>
      </a>
    </div>
  <% end %>
</div>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th> detail</th>
      <th> price</th>
      <th> quantity</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for product <- @products do %>
    <tr>
      <td><%= product.name %></td>
      <td><%= product.detail %></td>
      <td><%= product.price %></td>
      <td><%= product.quantity %></td>

      <td>
        <span><%= link "Show", to: Routes.product_path(@conn, :show, product) %></span>
        <span><%= link "Edit", to: Routes.product_path(@conn, :edit, product) %></span>
        <span><%= link "Delete", to: Routes.product_path(@conn, :delete, product), method: :delete, data: [confirm: "Are you sure?"] %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Product", to: Routes.product_path(@conn, :new) %></span>
